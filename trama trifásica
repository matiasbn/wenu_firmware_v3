  Serial.print(",i1=");
  Serial.print(end_current[1], 2);
  Serial.print(",p1=");
  Serial.print(end_active_power[1], 1);

  // End Of String Protocol for retro compatibility
  Serial.print("$");

  // Separator 
  Serial.print("_");

  // Phase C
